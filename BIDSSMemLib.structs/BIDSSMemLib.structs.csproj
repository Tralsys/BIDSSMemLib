	<PropertyGroup>
		<DebugType>pdbonly</DebugType>
		<DebugSymbols>true</DebugSymbols>
		<DefineConstants Condition="'$(Configuration)'=='Debug'">DEBUG;TRACE</DefineConstants>
	</PropertyGroup>


	<ItemGroup>
    <PackageReference Include="IAtsPI" Version="1.0.0.2" />
  </ItemGroup>

</Project>